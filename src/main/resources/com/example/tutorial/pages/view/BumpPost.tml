<html t:type="layout" title="Bump Post"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:j="tapestry-library:jquery">
<form t:type="form" class="full-form" t:id="registerForm"
	data-ajax="false" method="post">
	<t:If t:test="showCaptcha">
		<t:kaptchafield t:visible="true" image="kaptcha" t:id="fcaptcha" />
		<br />
		<t:kaptchaimage t:id="kaptcha" style="float:left;" />
	</t:If>
	<br style="clear:both;"/>
	 
	<input type='submit' class='btn' value='Register'
		style="clear: both;" />
</form>
</html>