<html t:type="layout" title="Admin Subcategory"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd">

<!-- <t:beaneditform object="subcategory"/> -->


<t:form t:id="loginForm">

	<t:errors />

	<t:label for="categoryName" />
	<select t:type="Select" t:model="categories" t:id="categoryName"
		size="1" />
	<br />
	<t:label for="subcategoryName" />
	<input t:type="TextField" t:id="subcategoryName" size="30" />
	<br />
	<t:label for="showAll">Show on homepage</t:label>
	<t:checkbox t:id="showAll"
		style="display:inline; margin-left:10px; vertical-align:middle;" />
	<br />
	<input type="submit" value="Create" style="margin-top: 15px;" />
</t:form>



</html>